<%= link_to "Dashboard", root_url %> &gt; <%= link_to "Agendas", agendas_url %> &gt; Inserir agenda

<%= form_for(@agenda) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :nome, "Nome:" %>
    <%= f.text_field :nome %>
  </p>
  <p>
    <%= f.label :mes_formatado, "MÃªs:" %>
    <%= f.text_field :mes_formatado, :size => 6 %>
  </p>
  <p>
    <%= f.label :template_agenda_id, "Template:" %>
	<%= collection_select(:template, :agenda_id, Agenda.all(:order => "id desc", :conditions => { :user_id => current_user.id }), :id, :nome, :prompt => "Nenhum") %>
  </p>
  <div id="pagamentos" style="display: none">
  </div>
  <p>
    <%= f.submit 'Inserir' %> ou <%= link_to 'Voltar', agendas_path %>
  </p>
<% end %>
